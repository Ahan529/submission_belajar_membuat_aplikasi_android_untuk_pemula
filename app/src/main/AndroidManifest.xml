<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dicoding.kulinerjawabarat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/jawabarat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/jawabarat"
        android:supportsRtl="true"
        android:theme="@style/Theme.KulinerJawaBarat">
        <activity android:name=".about.About"></activity>
        <activity android:name=".kuliner.KulinerDetail"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>